<%@ page pageEncoding="UTF-8"%>
<!-- 注册框 -->
<div class="content">
    <h3>极简&nbsp;云茶</h3>
    <p>个人日记&nbsp;System</p>

    <!-- 注册 -->
    <form class="form-horizontal" method="post" action="register">
        <div class="form-group">
            <div class="col-sm-8 col-sm-offset-2">
                <input class="form-control" name="name" type="text"
                    placeholder="名字">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-8 col-sm-offset-2">
                <input class="form-control" name="email" type="email"
                    placeholder="邮箱">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-8 col-sm-offset-2">
                <input class="form-control" name="password" type="password"
                    maxlength="16" placeholder="密码">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-8 col-sm-offset-2">
                <input class="form-control" name="confirmPass" type="password"
                    maxlength="16" placeholder="确认密码">
            </div>
        </div>
        
        <div class="form-group">
            <div class="col-sm-8 col-sm-offset-2">
                <a href="login">登录账号</a>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-8 col-sm-offset-2">
                <button class="btn center-block login-btn">注册</button>
            </div>
        </div>
    </form>
    <!-- 注册end -->
</div>
<!-- 注册框end -->
<div class="" style="margin-top:12%;height: 300px;">
</div>